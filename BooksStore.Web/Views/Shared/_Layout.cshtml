<!DOCTYPE html>
<html lang="en">
<head>    
    @await Html.PartialAsync("MetaPartial")
    @await Html.PartialAsync("LinkPartial")
    @await Html.PartialAsync("TitlePartial")
</head>
<body>

    <div class="wrapper">
        @await Html.PartialAsync("MenuPartial")

        <div class="content">
            @RenderBody()
        </div>

        @await Html.PartialAsync("FooterPartial")
    </div>

        @await Html.PartialAsync("SkriptsPartial")
        @RenderSection("Scripts", required: false)
</body>
</html>
